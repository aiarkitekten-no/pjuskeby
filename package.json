{
  "name": "pjuskeby",
  "version": "1.0.0",
  "description": "Pjuskeby - Interaktiv historiefortelling med AI, kart og podcast",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "api": "tsx server/index.ts",
    "api:dev": "tsx watch server/index.ts",
    "db:migrate": "tsx server/db/migrate.ts",
    "db:sync-mentions": "tsx scripts/sync-entity-mentions.ts",
    "validate:koblinger": "tsx scripts/validate-koblinger.ts",
    "validate:placeholders": "tsx scripts/lint-no-placeholder.ts",
    "validate:all": "npm run validate:koblinger && npm run validate:placeholders",
    "wipe:dry-run": "tsx scripts/wipe-dynamic-data.ts --dry-run",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.7",
    "@astrojs/node": "^9.5.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "@runware/sdk-js": "^1.1.47",
    "@types/bcrypt": "^6.0.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/winston": "^2.4.4",
    "astro": "^5.14.5",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.6.1",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.29.5",
    "fastify": "^4.29.1",
    "ioredis": "^5.8.1",
    "meilisearch": "^0.53.0",
    "mysql2": "^3.15.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "glob": "^10.3.0",
    "husky": "^8.0.0",
    "lighthouse": "^13.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
